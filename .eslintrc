{
  "env": {
    "browser": true
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "vue"
  ],
  "extends": ["standard", "plugin:vue/strongly-recommended"],
  "rules": {
    "no-console": 1,
    "prefer-template": 2,
    "semi": ["error", "always"],
    "space-before-function-paren": 0,
    "no-useless-constructor": 0
  }
}
